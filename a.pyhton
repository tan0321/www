import random
 
  

a=int(input('(ぐー:1,ちょき:2,ぱー:3)あなたが出す手は？:'))
b=random.randint(1,3)
if a==b:
   print("あいこです")
elif a==1 and b==2:
   print("あなたの勝ちです")
elif a==1 and b==3:
       print("あなたの負けです")
elif a==2 and b==1:
  print("あなたの負けです")
elif a==2 and b==3:
    print("あなたの勝ちです")
elif a==3 and b==1:
  print("あなたの勝ちです")
elif a==3 and b==2:
  print("あなたの負けです")
elif a<1 or 3<a:
  print("1,2,3から選んでください")
